Definition()
{
    OnThen->Store("dir") {
        OnThen->Template():PrintContent(f"opendir.html")
            | Case("subdirs"):Load("dir"):Dir() {
                OnThen->Store("dirs"):Call(f"dirs.rkop");
            } 
            | Case("subfiles"):Load("dir"):Dir() {
                OnElse->Store("files"):Call(f"files.rkop");
            }; 
        };
    };
}